{
  "expo": {
    "name": "Pancake Theory",
    "slug": "pancake-theory",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/build/ios-light.png",
    "scheme": "pancaketheory",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": false,
      "icon": {
        "light": "./assets/build/ios-light.png",
        "dark": "./assets/build/ios-dark.png",
        "tinted": "./assets/build/ios-tinted.png"
      },
      "bundleIdentifier": "com.keithkurak.pancaketheory",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/build/adaptive-icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.keithkurak.pancaketheory"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/build/splash-icon-light.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "image": "./assets/build/splash-icon-dark.png",
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "1de013cf-b8b2-4ac3-9e4d-dd70bfd4892e",
        "test": 3
      },
      "someProp": "whatev"
    },
    "owner": "expo-production",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/1de013cf-b8b2-4ac3-9e4d-dd70bfd4892e"
    }
  }
}
